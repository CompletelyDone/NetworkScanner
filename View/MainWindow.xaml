<Window x:Class="View.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:View"
        mc:Ignorable="d"
        Title="NetworkScanner" Height="600" Width="300">
    <Grid>
        <Menu>
            <MenuItem Header="Main"/>
        </Menu>
        <StackPanel Orientation="Vertical">
            <ComboBox Height="35">
                <TextBlock>Tom</TextBlock>
                <TextBlock>Bob</TextBlock>
                <TextBlock>Sam</TextBlock>
                <TextBlock>Alice</TextBlock>
            </ComboBox>
            <Button Content="|>" Height="200" IsDefault="False" IsCancel="False"/>
            <TextBlock Text="Компьютеров в сети: "/>
            <TextBlock Text="Компьютеров в сети: "/>
            <TextBlock Text="Компьютеров в сети: "/>
        </StackPanel>
    </Grid>
</Window>
